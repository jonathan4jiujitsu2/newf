oEditor.CreateBox Array("NAME:BoxParameters", _
"XPosition:=", res3_x + 27.165 & "mil", _
"YPosition:=", res3_y + 27.165 & "mil", _
"ZPosition:=", res3_z & "mil", _
"XSize:=", res_typeB_slot_width & "mil", _
