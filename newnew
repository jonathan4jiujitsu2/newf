'Subtract both slot parts
oEditor.Subtract Array("NAME:Selections", _
"Blank Parts:=", "Resonator7_Main", _
"Tool Parts:=", "Resonator7_Slot1,Resonator7_Slot2"), _
Array("NAME:SubtractParameters", _
"KeepOriginals:=", false)

